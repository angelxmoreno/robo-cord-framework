{
    "name": "@robo-cord/framework",
    "type": "module",
    "main": "./src/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "bun": "./src/index.ts",
            "import": "./dist/index.js",
            "types": "./src/index.ts"
        },
        "./schemas": {
            "bun": "./src/schemas/index.ts",
            "import": "./dist/schemas/index.js",
            "types": "./src/schemas/index.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf node_modules bun.lock dist",
        "test": "bun test",
        "typecheck": "tsc --noEmit",
        "prebuild": "rimraf dist",
        "build": "tsc --project tsconfig.build.json"
    },
    "engines": {
        "node": ">=18.17.0",
        "bun": ">=1.0.0"
    },
    "dependencies": {
        "dotenv": "^17.2.0",
        "pg": "^8.16.3",
        "pino": "^9.8.0",
        "pino-pretty": "^13.1.1",
        "reflect-metadata": "^0.2.2",
        "tsyringe": "^4.8.0",
        "typeorm": "^0.3.25",
        "zod": "^4.0.0"
    },
    "devDependencies": {
        "@types/bun": "^1.2.0",
        "discord.js": "^14.21.0",
        "rimraf": "^6.0.0",
        "tsc-alias": "^1.8.0"
    },
    "peerDependencies": {
        "discord.js": "^14.21.0",
        "typescript": "^5"
    }
}
